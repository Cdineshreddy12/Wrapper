{
  "name": "wrapper-backend",
  "version": "1.0.0",
  "description": "Backend API for wrapper application",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "build": "echo 'No build step required for JavaScript'",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push:pg",
    "db:introspect": "drizzle-kit introspect:pg",
    "migrate": "node scripts/run-migrations.js migrate",
    "sync-permissions": "node src/scripts/sync-permissions.js sync",
    "sync-permissions:validate": "node src/scripts/sync-permissions.js validate",
    "sync-permissions:summary": "node src/scripts/sync-permissions.js summary",
    "sync-permissions:dry-run": "node src/scripts/sync-permissions.js dry-run",
    "sync-permissions:app": "node src/scripts/sync-permissions.js app"
  },
  "dependencies": {
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.1",
    "fastify": "^4.24.3",
    "inquirer": "^12.6.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "node-cron": "^4.1.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "postgres": "^3.4.3",
    "recharts": "^2.15.3",
    "redis": "^4.6.12",
    "stripe": "^14.8.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "winston-elasticsearch": "^0.15.9"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2"
  }
}
