{
  "name": "wrapper-backend",
  "version": "1.0.0",
  "description": "Backend API for wrapper application",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/bootstrap.ts",
    "start": "node dist/bootstrap.js",
    "start:dev": "tsx src/bootstrap.ts",
    "install:full": "rm -rf node_modules && pnpm install",
    "start:check": "node src/scripts/check-startup.mjs",
    "dev:check": "node src/scripts/check-startup.mjs",
    "test:db": "node src/scripts/test-db.mjs",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push:pg",
    "db:introspect": "drizzle-kit introspect:pg",
    "migrate:drop-credit-allocation-tables": "node src/scripts/run-credit-allocation-migration.js",
    "migrate:cleanup-tenant-data": "node src/scripts/cleanup-tenant-data-for-migration.js",
    "migrate:create-notification-templates": "node src/scripts/create-notification-templates-table.js",
    "migrate:create-external-applications": "node src/scripts/create-external-applications-table.js",
    "migrate:add-ui-customization": "node src/scripts/add-ui-customization-fields.js",
    "migrate:all": "bash src/scripts/run-all-migrations.sh",
    "delete-tenant": "node src/scripts/delete-tenant-data.js",
    "delete-all-tenants": "node src/scripts/delete-all-tenant-data.js",
    "sync-permissions": "node src/scripts/sync-permissions.js sync",
    "sync-permissions:validate": "node src/scripts/sync-permissions.js validate",
    "sync-permissions:summary": "node src/scripts/sync-permissions.js summary",
    "sync-permissions:dry-run": "node src/scripts/sync-permissions.js dry-run",
    "sync-permissions:app": "node src/scripts/sync-permissions.js app",
    "temporal:worker": "node temporal/worker.js",
    "temporal:start": "node temporal/start-workflow.js",
    "amazon-mq:setup-topology": "node ../../scripts/setup-amazon-mq-topology.js",
    "lint": "eslint src/",
    "ngrok": "ngrok http ${PORT:-3000}"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-route-53": "^3.700.0",
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@temporalio/worker": "^1.8.0",
    "@temporalio/workflow": "^1.8.0",
    "amqplib": "^0.10.3",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.1",
    "fastify": "^4.24.3",
    "fastify-type-provider-zod": "^2.1.0",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.1.0",
    "nodemailer": "^6.9.7",
    "openai": "^4.104.0",
    "postgres": "^3.4.3",
    "stripe": "^14.8.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-elasticsearch": "^0.15.9",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.0",
    "@types/nodemailer": "^7.0.10",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
