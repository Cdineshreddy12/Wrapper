{
  "name": "wrapper-backend",
  "version": "1.0.0",
  "description": "Backend API for wrapper application",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "build": "echo 'No build step required for JavaScript'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push:pg",
    "db:introspect": "drizzle-kit introspect:pg",
    "migrate": "node scripts/run-migrations.js migrate",
    "migrate:drop-credit-allocation-tables": "node src/scripts/run-credit-allocation-migration.js",
    "migrate:cleanup-tenant-data": "node src/scripts/cleanup-tenant-data-for-migration.js",
    "migrate:create-notification-templates": "node src/scripts/create-notification-templates-table.js",
    "migrate:create-external-applications": "node src/scripts/create-external-applications-table.js",
    "migrate:add-ui-customization": "node src/scripts/add-ui-customization-fields.js",
    "migrate:all": "bash src/scripts/run-all-migrations.sh",
    "delete-tenant": "node src/scripts/delete-tenant-data.js",
    "sync-permissions": "node src/scripts/sync-permissions.js sync",
    "sync-permissions:validate": "node src/scripts/sync-permissions.js validate",
    "sync-permissions:summary": "node src/scripts/sync-permissions.js summary",
    "sync-permissions:dry-run": "node src/scripts/sync-permissions.js dry-run",
    "sync-permissions:app": "node src/scripts/sync-permissions.js app",
    "temporal:worker": "node temporal/worker.js",
    "temporal:start": "node temporal/start-workflow.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@temporalio/client": "^1.8.0",
    "@temporalio/worker": "^1.8.0",
    "@temporalio/workflow": "^1.8.0",
    "@temporalio/activity": "^1.8.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.66.2",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.1",
    "fastify": "^4.24.3",
    "inquirer": "^12.6.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "node-cron": "^4.1.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "openai": "^4.104.0",
    "postgres": "^3.4.3",
    "recharts": "^2.15.3",
    "redis": "^4.6.12",
    "stripe": "^14.8.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-elasticsearch": "^0.15.9",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  }
}
